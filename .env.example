
# Rename this file to .env.local and fill in your secret values.
# This file should be in your .gitignore to prevent committing secrets.

# Resend API Key for sending contact form emails
# Get yours from https://resend.com/api-keys
RESEND_API_KEY="your_resend_api_key"

# Cloudinary credentials for media uploads
# Get these from your Cloudinary dashboard settings.
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME="your_cloud_name"
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET="your_upload_preset"
